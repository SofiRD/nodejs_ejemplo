<!DOCTYPE html>
<html lang="en">
<head>
  <!-- Bootstrap CSS -->
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">

  <title>DB data</title>
</head>
<body>

    <nav class="navbar navbar-expand-lg navbar-dark bg-dark">
  <div class="container-fluid">
    <a class="navbar-brand active" href="index.html">Home</a>
    <div class="collapse navbar-collapse" id="navbarSupportedContent">
      <ul class="navbar-nav me-auto mb-2 mb-lg-0">
        <li class="nav-item dropdown">
          
        </li>
      </ul>      
    </div>
  </div>
</nav>
    <div class="table-data">
<h2>E-shop data <span class="badge bg-secondary">New</span></h2>

    <table border="1" class="table table-striped">
        <tr>
            <th>id_category</th>
            <th>id_parent</th>
            <th>id_shop_default</th>
            <th>level_depth</th>
            <th>active</th>
            <th>date_add</th>
            <th>date_upd</th>
            <th>name</th>
        </tr>
        
        <%
        if(userData.length!=0){
        var i=1;
        userData.forEach(function(data){
        %>
        <tr>
            <td><%=data.id_category; %></td>
            <td><%=data.id_parent %></td>
            <td><%=data.id_shop_default %></td>
            <td><%=data.level_depth %></td>
            <td><%=data.active %></td>
            <td><%=data.date_add %></td>
            <td><%=date_upd %></td>
            <td><%=data.name %></td>
        </tr>
        <%  i++; }) %>
        <% } else{ %>
            <tr>
                <td colspan="8">No Data Found</td>
        <% } %>
	    <td><span class="placeholder col-4"></span></td>
            <td><span class="placeholder col-4"></span></td>
            <td><span class="placeholder col-4"></span></td>
            <td><span class="placeholder col-4"></span></td>
            <td><span class="placeholder col-4"></span></td>
            <td><span class="placeholder col-4"></span></td>
            <td><span class="placeholder col-4"></span></td>
            <td><span class="placeholder col-4"></span></td>

            </tr>
    </table>
    </div>
</body>
</html>